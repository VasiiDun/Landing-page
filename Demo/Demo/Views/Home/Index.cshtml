@model IEnumerable<Entity.Models.Order>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClientData.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientData.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientData.LastName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelitem => item.ClientData.Id)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.ClientData.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.ClientData.LastName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
